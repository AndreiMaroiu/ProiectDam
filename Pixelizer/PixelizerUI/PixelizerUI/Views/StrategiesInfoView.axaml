<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
			 xmlns:vm="using:PixelizerUI.ViewModels"
			 xmlns:icons="clr-namespace:Material.Icons.Avalonia;assembly=Material.Icons.Avalonia"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="PixelizerUI.Views.StrategiesInfoView"
			 >
	<Design.DataContext>
		<vm:StrategiesInfoViewModel></vm:StrategiesInfoViewModel>
	</Design.DataContext>

	<Panel>
		<TabControl TabStripPlacement="Left" Padding="25" HorizontalContentAlignment="Center" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
			<!--<TabControl.Styles>
			<Style Selector="TabControl WrapPanel">
				<Setter Property="Background" Value="#15151501"></Setter>
			</Style>
		</TabControl.Styles>-->
			<TabItem>
				<TabItem.Header>
					<StackPanel Orientation="Horizontal" Spacing="5">
						<icons:MaterialIcon Kind="MaterialUi"></icons:MaterialIcon>
						<TextBlock>Average</TextBlock>
					</StackPanel>
				</TabItem.Header>
				<TabItem.Content>
					The output pixel is the average of the block
				</TabItem.Content>
			</TabItem>
			<TabItem Header="Most Common">
				The most commom color in the block will be the output pixel
			</TabItem>
		</TabControl>
	</Panel>
</UserControl>
